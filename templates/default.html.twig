{# templates/default.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}Nerdwolf{% endblock %}

{% block body %}
	{% for label, messages in app.flashes %}
    		{% for message in messages %}
        		<div class="flash-{{ label }}">
           			{{ message }}
		        </div>
		{% endfor %}
	{% endfor %}
	<div class="logo"><img src="/build/images/nerdwolf_grey_178x178.png"/></div>
	<div class="test">Nerdwolf random number: {{ test }}</div>
{% endblock %}
